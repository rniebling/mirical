<?xml version="1.0" encoding="UTF-8" ?>
<weblets-config xmlns="http://weblets.dev.java.net/config">
	<weblet>
		<weblet-name>de.sunrise.web</weblet-name>
		<weblet-class>
			net.java.dev.weblets.packaged.PackagedWeblet
		</weblet-class>
		<init-param>
			<param-name>package</param-name>
			<param-value>
				de.subito.sunrise.web
			</param-value>
		</init-param>
	</weblet>
	<weblet>
		<weblet-name>de.sunrise.applets</weblet-name>
		<weblet-class>
			net.java.dev.weblets.packaged.PackagedWeblet
		</weblet-class>
		<init-param>
			<param-name>package</param-name>
			<param-value>
				applets
			</param-value>
		</init-param>
	</weblet>
	<weblet>
		<weblet-name>de.sunrise.icons</weblet-name>
		<weblet-class>
			de.subito.sunrise.webapp.weblets.SunrisePackagedWeblet
		</weblet-class>
		<init-param>
			<param-name>package</param-name>
			<param-value>
				icons
			</param-value>
		</init-param>
	</weblet>
	<weblet>
		<weblet-name>de.sunrise.css</weblet-name>
		<weblet-class>
			de.subito.sunrise.webapp.weblets.SunrisePackagedWeblet
		</weblet-class>
		<init-param>
			<param-name>package</param-name>
			<param-value>
				css
			</param-value>
		</init-param>
	</weblet>
	<weblet>
		<weblet-name>de.sunrise.js</weblet-name>
		<weblet-class>
			de.subito.sunrise.webapp.weblets.SunrisePackagedWeblet
		</weblet-class>
		<init-param>
			<param-name>package</param-name>
			<param-value>
				js
			</param-value>
		</init-param>
	</weblet>

	<weblet-mapping>
		<weblet-name>de.sunrise.web</weblet-name>
		<url-pattern>/resources/*</url-pattern>
	</weblet-mapping>
	<weblet-mapping>
		<weblet-name>de.sunrise.applets</weblet-name>
		<url-pattern>/applets/*</url-pattern>
	</weblet-mapping>
	<weblet-mapping>
		<weblet-name>de.sunrise.icons</weblet-name>
		<url-pattern>/icons/*</url-pattern>
	</weblet-mapping>
	<weblet-mapping>
		<weblet-name>de.sunrise.css</weblet-name>
		<url-pattern>/css/*</url-pattern>
	</weblet-mapping>
	<weblet-mapping>
		<weblet-name>de.sunrise.js</weblet-name>
		<url-pattern>/js/*</url-pattern>
	</weblet-mapping>
</weblets-config>